        "\n",
        "num = random.randint(1, 100)\n",
        "\n",
        "if num % 3 == 0 and num % 5 != 0:\n",
        "    print(\"Fizz\")\n",
        "elif num % 5 == 0 and num % 3 != 0:\n",
        "    print(\"Buzz\")\n",
        "elif num % 15 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "else:\n",
        "    print(num)"
